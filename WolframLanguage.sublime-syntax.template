    - match: '16\^\^{{hexadecimal}}{{precision}}\*\^'
      scope: invalid.illegal
    - match: '16\^\^{{hexadecimal}}{{precision}}'
      scope: constant.numeric.wolfram
    - match: '16\^\^{{hexadecimal}}{{exponent}}'
      scope: constant.numeric.wolfram
    - match: '16\^\^{{hexadecimal}}\*\^'
      scope: invalid.illegal
    - match: '16\^\^{{hexadecimal}}'
      scope: constant.numeric.wolfram
    - match: '16\^\^'
      scope: invalid.illegal
    - match: '{{decimal}}{{accuracy}}{{exponent}}'
      scope: constant.numeric.wolfram
    - match: '{{decimal}}{{accuracy}}\*\^'
      scope: invalid.illegal
    - match: '{{decimal}}{{accuracy}}'
      scope: constant.numeric.wolfram
    - match: '{{decimal}}``'
      scope: invalid.illegal
    - match: '{{decimal}}{{precision}}{{exponent}}'
      scope: constant.numeric.wolfram
    - match: '{{decimal}}{{precision}}\*\^'
      scope: invalid.illegal
    - match: '{{decimal}}{{precision}}'
      scope: constant.numeric.wolfram
    - match: '{{decimal}}{{exponent}}'
      scope: constant.numeric.wolfram
    - match: '{{decimal}}\*\^'
      scope: invalid.illegal
    - match: '{{decimal}}'
      scope: constant.numeric.wolfram

  raw_strings:
    - match: \"\"\"
      scope: punctuation.definition.rawstring.begin
      push:
        - meta_scope: string.quoted.triple.wolfram
        - meta_include_prototype: false
        - match: \"\"\"
          scope: punctuation.definition.string.end
          pop: true
        - match: .

  strings:
    - match: \"
      scope: punctuation.definition.string.begin
      push:
        - meta_scope: string.quoted.double.wolfram
        - meta_include_prototype: false
        - match: \"
          scope: punctuation.definition.string.end
          pop: true
        - include: string_escaped_characters
        - match: .

  stringifying_operators:
    - match: (>>>)(?=\s*")
      captures:
        1: keyword.operator.PutAppend.wolfram
    - match: (>>>)\s*(\w+)
      captures:
        1: keyword.operator.PutAppend.wolfram
        2: string.unquoted.wolfram
    - match: '>>>'
      scope: invalid.illegal
    - match: (::)(?=")
      captures:
        1: keyword.operator.MessageName.wolfram
    - match: (::)([[:alpha:]][[:alnum:]]*)
      captures:
        1: keyword.operator.MessageName.wolfram
        2: string.unquoted.wolfram
    - match: '::'
      scope: invalid.illegal
    - match: (<<)(?=\s*")
      captures:
        1: keyword.operator.Get.wolfram
    - match: (<<)\s*([`[:alpha:]][`[:alnum:]]*)
      captures:
        1: keyword.operator.Get.wolfram
        2: string.unquoted.wolfram
    - match: '<<'
      scope: invalid.illegal
    - match: (>>)(?=\s*")
      captures:
        1: keyword.operator.Put.wolfram
    - match: (>>)\s*(\w+)
      captures:
        1: keyword.operator.Put.wolfram
        2: string.unquoted.wolfram
    - match: '>>'
      scope: invalid.illegal

  operators:
    - match: (?:\^:=)
      scope: keyword.operator.assignment.UpSetDelayed.wolfram
    - match: (?:\^:)
      scope: invalid.illegal
    - match: (?:'=)
      scope: invalid.printing
    - match: (?:')
      scope: invalid.printing
    - match: (?:===)
      scope: keyword.operator.SameQ.wolfram
    - match: (?:=!=|\.\.\.|//\.|@@@|<->|//@|=>|~!~|~~)
      scope: keyword.operator.wolfram
    - match: (?:\|->)
      scope: keyword.operator.Function.wolfram
    - match: (?://=)
      scope: keyword.operator.assignment.ApplyTo.wolfram
    - match: (?:--|\+\+)
      scope: keyword.operator.arithmetic.wolfram
    - match: (?:\|\||&&)
      scope: keyword.operator.logical.wolfram
    - match: (?::=)
      scope: keyword.operator.assignment.SetDelayed.wolfram
    - match: (?:\^=)
      scope: keyword.operator.assignment.UpSet.wolfram
    - match: (?:/=)
      scope: keyword.operator.assignment.DivideBy.wolfram
    - match: (?:\+=)
      scope: keyword.operator.assignment.AddTo.wolfram
    - match: (?:=\s+\.(?![0-9]))
      scope: invalid.whitespace.Unset.wolfram
    - match: (?:=\.(?![0-9]))
      scope: keyword.operator.assignment.Unset.wolfram
    - match: (?:\*=)
      scope: keyword.operator.assignment.TimesBy.wolfram
    - match: (?:-=)
      scope: keyword.operator.assignment.SubtractFrom.wolfram
    - match: (?:/:)
      scope: keyword.operator.assignment.Tag.wolfram
    - match: (?:;;)$
      scope: invalid.endofline.Span.wolfram
    - match: (?:;;)
      scope: keyword.operator.Span.wolfram
    - match: (?:!=)
      scope: keyword.operator.Unequal.wolfram
    - match: (?:==)
      scope: keyword.operator.Equal.wolfram
    - match: (?:!!)
      scope: keyword.operator.BangBang.wolfram
    - match: (?:\?\?)
      scope: invalid.illegal.Information.wolfram
    - match: (?:<=|>=|\.\.|:>|<>|->|/@|/;|/\.|//|/\*|@@|@\*|~~|\*\*)
      scope: keyword.operator.wolfram
    - match: (?:-|\+|/|\*)
      scope: keyword.operator.arithmetic.wolfram
    - match: (?:=)
      scope: keyword.operator.assignment.Set.wolfram
    - match: (?:<)
      scope: keyword.operator.Less.wolfram
    - match: (?:\|)
      scope: keyword.operator.Alternatives.wolfram
    - match: (?:!)
      scope: keyword.operator.Bang.wolfram
    - match: (?:;)
      scope: keyword.operator.CompoundExpression.wolfram punctuation.terminator
    - match: (?:,)
      scope: keyword.operator.Comma.wolfram punctuation.separator
    - match: ^(?:\?)
      scope: invalid.startofline.Information.wolfram
    - match: (?:\?)
      scope: keyword.operator.PatternTest.wolfram
    - match: (?:')
      scope: keyword.operator.Derivative.wolfram
    - match: (?:&)
      scope: keyword.operator.Function.wolfram
    - match: (?:>|\^|\.|:|@|~)
      scope: keyword.operator.wolfram

  symbols:

$SYMBOLS$

    - match: '{{symbolSegment}}(?:`{{symbolSegment}})+(?=\s*(\[(?!\s*\[)|@(?!@)))'
      scope: variable.function.wolfram
    - match: '{{symbolSegment}}(?:`{{symbolSegment}})+'
      scope: symbol.unrecognized.wolfram
    - match: '{{symbolSegment}}`'
      scope: invalid.illegal.wolfram
    - match: '(?:`{{symbolSegment}})+(?=\s*(\[(?!\s*\[)|@(?!@)))'
      scope: variable.function.wolfram
    - match: '(?:`{{symbolSegment}})+'
      scope: symbol.unrecognized.wolfram
    - match: '`'
      scope: invalid.illegal.wolfram

    - match: '{{obsoleteSymbols_A}}{{symbolEndBoundary}}'
      scope: invalid.deprecated.wolfram
    - match: '{{obsoleteSymbols_B}}{{symbolEndBoundary}}'
      scope: invalid.deprecated.wolfram
    - match: '{{obsoleteSymbols_C}}{{symbolEndBoundary}}'
      scope: invalid.deprecated.wolfram
    - match: '{{obsoleteSymbols_D}}{{symbolEndBoundary}}'
      scope: invalid.deprecated.wolfram
    - match: '{{obsoleteSymbols_E}}{{symbolEndBoundary}}'
      scope: invalid.deprecated.wolfram
    - match: '{{obsoleteSymbols_F}}{{symbolEndBoundary}}'
      scope: invalid.deprecated.wolfram
    - match: '{{obsoleteSymbols_G}}{{symbolEndBoundary}}'
      scope: invalid.deprecated.wolfram
    - match: '{{obsoleteSymbols_H}}{{symbolEndBoundary}}'
      scope: invalid.deprecated.wolfram
    - match: '{{obsoleteSymbols_I}}{{symbolEndBoundary}}'
      scope: invalid.deprecated.wolfram
    - match: '{{obsoleteSymbols_L}}{{symbolEndBoundary}}'
      scope: invalid.deprecated.wolfram
    - match: '{{obsoleteSymbols_M}}{{symbolEndBoundary}}'
      scope: invalid.deprecated.wolfram
    - match: '{{obsoleteSymbols_N}}{{symbolEndBoundary}}'
      scope: invalid.deprecated.wolfram
    - match: '{{obsoleteSymbols_O}}{{symbolEndBoundary}}'
      scope: invalid.deprecated.wolfram
    - match: '{{obsoleteSymbols_P}}{{symbolEndBoundary}}'
      scope: invalid.deprecated.wolfram
    - match: '{{obsoleteSymbols_R}}{{symbolEndBoundary}}'
      scope: invalid.deprecated.wolfram
    - match: '{{obsoleteSymbols_S}}{{symbolEndBoundary}}'
      scope: invalid.deprecated.wolfram
    - match: '{{obsoleteSymbols_T}}{{symbolEndBoundary}}'
      scope: invalid.deprecated.wolfram
    - match: '{{obsoleteSymbols_U}}{{symbolEndBoundary}}'
      scope: invalid.deprecated.wolfram
    - match: '{{obsoleteSymbols_V}}{{symbolEndBoundary}}'
      scope: invalid.deprecated.wolfram
    - match: '{{obsoleteSymbols_W}}{{symbolEndBoundary}}'
      scope: invalid.deprecated.wolfram
    - match: '{{obsoleteSymbols_DOLLAR}}{{symbolEndBoundary}}'
      scope: invalid.deprecated.wolfram

    # - match: '{{symbolSegment}}(?=\s*(\[(?!\s*\[)|@(?!@)))'
    #   scope: variable.function.wolfram

    - match: '[$][A-Z]{{symbolSegment}}'
      scope: variable.global.unrecognized.wolfram

    - match: '[$]{{symbolSegment}}'
      scope: variable.global.wolfram

    - match: '[A-Z]{{symbolSegment}}'
      scope: variable.local.unrecognized.wolfram

    - match: '{{symbolSegment}}[:_]'
      scope: variable.local.pattern.wolfram

    - match: '{{symbolSegment}}'
      scope: variable.local.wolfram

  groups:
    - match: \\\)
      scope: invalid.illegal.stray-linearsyntaxparens-end.wolfram
    - match: \)
      scope: invalid.illegal.stray-parens-end.wolfram
    - match: \[\s+\[
      scope: invalid.whitespace.Part.wolfram
    - match: \]\s+\]
      scope: invalid.whitespace.Part.wolfram
    - match: \]\]
      scope: invalid.illegal.stray-parts-end.wolfram
    - match: \]
      scope: invalid.illegal.stray-brackets-end.wolfram
    - match: \}
      scope: invalid.illegal.stray-braces-end.wolfram
    - match: \|>
      scope: invalid.illegal.stray-associations-end.wolfram
    - include: linearsyntaxparen_group
    - include: paren_group
    - include: part_group
    - include: bracket_group
    - include: brace_group
    - include: association_group

  linearsyntaxparen_group:
    - match: \\\(
      scope: punctuation.section.linearsyntaxparens.begin.wolfram
      push:
        - meta_scope: meta.linearsyntaxparens.wolfram
        - match: \\\)
          scope: punctuation.section.linearsyntaxparens.end.wolfram
          pop: true
        - include: expressions

  paren_group:
    - match: \(
      scope: punctuation.section.parens.begin.wolfram
      push:
        - meta_scope: meta.parens.wolfram
        - match: \)
          scope: punctuation.section.parens.end.wolfram
          pop: true
        - include: expressions

  part_group:
    - match: \[\[
      scope: punctuation.section.parts.begin.wolfram
      push:
        - meta_scope: meta.parts.wolfram
        - match: \]\]
          scope: punctuation.section.parts.end.wolfram
          pop: true
        - include: expressions

  bracket_group:
    - match: ::\[|\[
      scope: punctuation.section.brackets.begin.wolfram
      push:
        - meta_scope: meta.brackets.wolfram
        - match: \]
          scope: punctuation.section.brackets.end.wolfram
          pop: true
        - include: expressions

  brace_group:
    - match: \{
      scope: punctuation.section.braces.begin.wolfram
      push:
        - meta_scope: meta.braces.wolfram
        - match: \}
          scope: punctuation.section.braces.end.wolfram
          pop: true
        - include: expressions

  association_group:
    - match: <\|
      scope: punctuation.section.associations.begin.wolfram
      push:
        - meta_scope: meta.associations.wolfram
        - match: \|>
          scope: punctuation.section.associations.end.wolfram
          pop: true
        - include: expressions

  pattern_operators:
    - match: ___
      scope: keyword.operator.BlankNullSequence.wolfram
    - match: __
      scope: keyword.operator.BlankSequence.wolfram
    - match: _\.
      scope: keyword.operator.Optional.wolfram
    - match: _
      scope: keyword.operator.Blank.wolfram

  out:
    - match: '%\d+'
      scope: keyword.other.Out.wolfram
    - match: '%+'
      scope: keyword.other.Out.wolfram

  slot:
    - match: '#[[:alpha:]][[:alnum:]]*'
      scope: keyword.other.Slot.wolfram
    - match: '##\d*'
      scope: keyword.other.SlotSequence.wolfram
    - match: '#\d*'
      scope: keyword.other.Slot.wolfram
