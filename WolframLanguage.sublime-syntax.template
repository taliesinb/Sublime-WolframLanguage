  prototype:
    - include: comments

  shebang:
    - match: '\A(#!).*(?=$)'
      scope: comment.line.shebang.wolfram
      captures:
        1: punctuation.definition.comment.wolfram

  simple_toplevel_definitions:
    - match: ^\s*(?:Attributes|Format|Options)\s*(\[)({{symbolSegment}})(\])\s*(:=|=(?!!|=|\.))
      captures:
        1: punctuation.section.brackets.begin.wolfram
        2: meta.function.wolfram entity.name.function.wolfram
        3: punctuation.section.brackets.end.wolfram
        4: keyword.operator.assignment.wolfram
    - match: ^\s*({{symbolSegment}})(?=(\[(?>[^\[\]]+|\g<-1>)*\])\s*(?:/;.*)?(?::=|=(?!!|=|\.)))
      captures:
        1: meta.function.wolfram entity.name.function.wolfram
    - match: ^\s*({{symbolSegment}})(?=(\[(?>[^\[\]]+|\g<-1>)*\])(\[(?>[^\[\]]+|\g<-1>)*\])\s*(?:/;.*)?(?::=|=(?!!|=|\.)))
      captures:
        1: meta.function.wolfram entity.name.function.wolfram
    - match: ^\s*({{symbolSegment}})(?=\s*(?:/;.*)?(?::=|=) (?:Case))
      captures:
        1: meta.function.wolfram entity.name.function.wolfram
    - match: ^\s*(\${{symbolSegment}})(?=\s*(?:/;.*)?(?::=|=(?!!|=|\.)))
      captures:
        1: meta.function.wolfram entity.name.constant.wolfram variable.global.wolfram
    - match: ^\s*({{symbolSegment}})(?=\s*(?:/;.*)?(?::=|=(?!!|=|\.)))
      captures:
        1: meta.function.wolfram entity.name.constant.wolfram variable.local.wolfram

  package_items:
    - match: '(?:{{Package_P}}|{{Package_S}})\['
      scope: meta.package.wolfram
      push: package_items_block

  package_items_block:
    - meta_scope: meta.package.wolfram
    - match: ']'
      pop: true
    - match: ','
    - match: '({{symbolSegment}})[, \n\t]*'
      captures:
        1: entity.name.function.package.item.wolfram
    - match: '.'
      scope: invalid.illegal

  expressions:
    - include: escaped_character_symbols
    - include: escaped_characters
    - include: out
    - include: slot
    - include: literals
    - include: groups
    - include: stringifying_operators
    - include: operators
    - include: pattern_operators
    - include: symbols
    - match: '{{punctuationChar}}'
      scope: invalid.illegal

  comments:
    - match: \(\*
      scope: punctuation.definition.comment.wolfram
      push: comment-block
    - match: \*\)
      scope: invalid.illegal.stray-comment-end.wolfram

  comment-block:
    - meta_scope: comment.block.wolfram
    - match: \(\*
      scope: punctuation.definition.comment.wolfram
      push: comment-block
    - match: \*\)
      scope: punctuation.definition.comment.wolfram
      pop: true
    - match: .

  escaped_character_symbols:
    - match: System`\\\[{{systemLongNames_F}}\]{{symbolEndBoundary}}
      scope: constant.language.wolfram
    - match: System`\\\[{{systemLongNames_S}}\]{{symbolEndBoundary}}
