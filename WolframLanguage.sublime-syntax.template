    - match: \\\[{{symbolSegment}}?\]?
      scope: invalid.illegal.BadLongName
    - match: \\{{symbolSegment}}\]
      scope: invalid.illegal.BadLongName
    - match: \\:\h{4}
      scope: donothighlight.constant.character.escape
    - match: \\:\h{1,3}
      scope: invalid.illegal
    - match: \\\.\h{2}
